{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Statement1",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Statement2",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucketMultipartUploads",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:GetBucketAcl"
            ],
            "Resource": "arn:aws:s3:::engr-it"
        },
        {
            "Sid": "Statement3",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObjectAcl",
                "s3:GetObject",
                "s3:AbortMultipartUpload",
                "s3:DeleteObjectVersion",
                "s3:PutObjectVersionAcl",
                "s3:GetObjectVersionAcl",
                "s3:DeleteObject",
                "s3:PutObjectAcl",
                "s3:GetObjectVersion"
            ],
            "Resource": "arn:aws:s3:::engr-it/*"
        },
        {
            "Sid": "Statement4",
            "Effect": "Allow",
            "Action": [
                "rds:Describe*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Statement5",
            "Effect": "Allow",
            "Action": [
                "ec2:Describe*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Statement6",
            "Effect": "Allow",
            "Action": [
                "lambda:List*",
                "lambda:Get*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Statement7",
            "Effect": "Allow",
            "Action": [
                "glacier:List*",
                "glacier:Get*",
                "glacier:Describe*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Statement8",
            "Effect": "Allow",
            "Action": [
                "apigateway:List*",
                "apigateway:Get*"
            ],
            "Resource": "*"
        }
    ]
}